       className="com.android.vending.AssetBrowserActivity"            x="2"            y="4"/>        <!-- folder : Google -->        <folder            launcher:screen="0"            launcher:title="Google"            launcher:x="3"            launcher:y="4" >            <!-- Google App -->            <favorite                launcher:screen="0"                launcher:packageName="com.google.android.googlequicksearchbox"                launcher:className="com.google.android.googlequicksearchbox.SearchActivity" />            <!-- Chrome -->            <favorite                screen="1"                packageName="com.android.chrome"                className="com.google.android.apps.chrome.Main" />            <!-- Gmail -->            <favorite                launcher:screen="2"                launcher:packageName="com.google.android.gm"                launcher:className="com.google.android.gm.ConversationListActivityGmail" />            <!-- Maps -->            <favorite                launcher:screen="3"                launcher:packageName="com.google.android.apps.maps"                launcher:className="com.google.android.maps.MapsActivity" />            <!-- YouTube -->            <favorite                launcher:screen="4"                launcher:packageName="com.google.android.youtube"                launcher:className="com.google.android.youtube.app.honeycomb.Shell$HomeActivity" />            <!-- Drive -->            <favorite                launcher:screen="5"                launcher:packageName="com.google.android.apps.docs"                launcher:className="com.google.android.apps.docs.app.NewMainProxyActivity" />            <!-- Google Play Music -->            <favorite                launcher:screen="6"                launcher:packageName="com.google.android.music"                launcher:className="com.android.music.activitymanagement.TopLevelActivity" />            <!-- Google Play Movies & TV -->            <favorite                launcher:screen="7"                launcher:packageName="com.google.android.videos"                launcher:className="com.google.android.youtube.videos.EntryPoint" />            <!-- Duo -->            <favorite                launcher:screen="8"                launcher:packageName="com.google.android.apps.tachyon"                launcher:className="com.google.android.apps.tachyon.MainActivity" />            <!-- Photos -->            <favorite                launcher:screen="9"                launcher:packageName="com.google.android.apps.photos"                launcher:className="com.google.android.apps.photos.home.HomeActivity" />        </folder>	    <!-- Taxi Beat -->        <favorite             screen="1"             packageName="gr.androiddev.taxibeat"            className="com.taxibeat.passenger.clean_architecture.presentation.components.activities.ActLocate"	        x="0" 	        y="3" />		<!-- Disney Kingdom -->		<favorite 			screen="1"			x="1"			y="3"              			packageName="com.gameloft.android.GloftDMKF"			className="com.gameloft.android.GloftDMKF.Start"/>        <!-- Clock -->        <favorite            screen="1"            packageName="com.sec.android.app.clockpackage"            className="com.sec.android.app.clockpackage.ClockPackage"            x="0"            y="4"/>        <!-- Calendar -->        <favorite            screen="1"            packageName="com.samsung.android.calendar"            className="com.samsung.android.app.calendar.activity.MainActivity"            x="1"            y="4"/>        <!-- Game Launcher -->        <favorite            launcher:screen="1"            launcher:packageName="com.samsung.android.game.gamehome"            launcher:className="com.samsung.android.game.gamehome.activity.StartActivity"            x="2"            y="4"/>        <!-- folder : Aplicaciones de Microsoft -->        <folder            screen="1"            title="Aplicaciones de Microsoft"            x="3"            y="4">            <!-- Office Mobile -->            <favorite                screen="0"                packageName="com.microsoft.office.officehubrow"                className="com.microsoft.office.officesuite.OfficeSuiteActivity" />            <!-- OneDrive -->            <favorite                screen="1"                packageName="com.microsoft.skydrive"                className="com.microsoft.skydrive.MainActivity" />            <!-- LinkedIn -->            <favorite                screen="2"                packageName="com.linkedin.android"                className="com.linkedin.android.authenticator.LaunchActivity" />        </folder>    </home>    <hotseat>        <!-- Phone -->        <favorite            launcher:screen="0"            launcher:packageName="com.samsung.android.dialer"            launcher:className="com.samsung.android.dialer.DialtactsActivity" />        <!-- Messages -->        <favorite             screen="1"             packageName="com.samsung.android.messaging"             className="com.android.mms.ui.ConversationComposer" />        <!-- Samsung Internet -->        <favorite             screen="2"             packageName="com.sec.android.app.sbrowser"             className="com.sec.android.app.sbrowser.SBrowserMainActivity" />        <!-- Camera -->        <favorite             screen="3"            packageName="com.sec.android.app.camera"             className="com.sec.android.app.camera.Camera" />    </hotseat></favorites>